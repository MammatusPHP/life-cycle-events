{
    "name": "mammatus/life-cycle-events",
    "description": "♻️ Life Cycle Events for MammatusPHP Applications",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "react/promise": "^2.7",
        "wyrihaximus/constants": "^1.5"
    },
    "require-dev": {
        "wyrihaximus/broadcast": "dev-master",
        "wyrihaximus/test-utilities": "^2.2"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mammatus\\LifeCycleEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mammatus\\Tests\\LifeCycleEvents\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
